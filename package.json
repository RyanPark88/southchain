    },
    "dependencies": {
        "bootstrap": "4.5.0"
    },
    "devDependencies": {
        "autoprefixer": "9.8.0",
        "browser-sync": "2.26.7",
        "chokidar": "3.4.0",
        "concurrently": "5.2.0",
        "postcss": "7.0.32",
        "prettier": "2.0.5",
